package selfClass02;

//	화면

//	상품을 여러 개 주문한다.

//	상품 클래스
//	상품명
//	상품 가격
//	상품 재고

//	주문 클래스
//	주문 번호
//	상품들

//	주문할 때 결제할 총 금액을 계산할 수 있다.

public class Product02 {
	String name;
	int price;
	int stock;
	public Product02(String name, int price, int stock) {
		this.name = name;
		this.price = price;
		this.stock = stock;
	}
}
